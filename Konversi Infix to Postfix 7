#include <bits/stdc++.h>
using namespace std;

long unsigned int i;

bool Simbol(char a){
    if(a=='*' || a=='/' || a=='+' ||a=='-' || a=='%'){
        return true; 
    } else {    
        return false; 
    }
}

int operan(char a){ 
    if(a=='+' || a=='-'){ 
        return 1; 
    }
    if(a=='*' || a=='/' || a=='%'){ 
        return 2; 
    } 
    return 0;
}

int main(){
    string s;
    getline(cin, s);
    string baru_s="";
    
    for(i=0; i<s.length(); i++){
        if(s[i]==' '){
            continue;
        }else{
            baru_s+= s[i];    
        }        
    }
    //cout << "Print : " << konversi(baru_s);
    cout << konversi(baru_s);
    return 0;
}
