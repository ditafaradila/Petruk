#include<iostream>
#include<vector>
using namespace std;

int main()
{
    string input; 
    getline(cin,input);
    vector<char> eks;
    
    for(long unsigned int i=0; i<input.length(); i++){
        if(input[i] == ' ')
            continue;
        else if(isdigit(input[i])){ 
            while(i<input.length() && isdigit(input[i])){
                eks.push_back(input[i]);
                i++;
            }i--;
        }
        else if(input[i] == '-'){
            eks.push_back(input[i]);
            if(i==0){
                if(isdigit(input[i+1])){
                    i++;
                    while(i<input.length() && isdigit(input[i])){
                        eks.push_back(input[i]);
                        i++;
                    } i--;
                }
       else {
                    eks.push_back('1');
                    eks.push_back(' ');
                    eks.push_back('*');
                }
            } else {
                if(!isdigit(input[i-1]) && input[i+1] != ' '){
                    eks.push_back('1');
                    eks.push_back(' ');
                    eks.push_back('*');
                } 
            }
        } else
            eks.push_back(input[i]);
        eks.push_back(' ');
    }
    for(auto j:eks)
        cout << j;
  return 0;
}
